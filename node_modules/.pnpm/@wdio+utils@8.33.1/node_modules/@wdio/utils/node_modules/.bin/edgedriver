#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules/edgedriver/bin/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules/edgedriver/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules/edgedriver/bin/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules/edgedriver/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/edgedriver@5.5.0/node_modules:/Users/cyril/Workspace/codeceptjs-debug/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../edgedriver@5.5.0/node_modules/edgedriver/bin/edgedriver.js" "$@"
else
  exec node  "$basedir/../../../../../../edgedriver@5.5.0/node_modules/edgedriver/bin/edgedriver.js" "$@"
fi
